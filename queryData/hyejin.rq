################################################# 
# CQ7. 2015년 아이유가 프로듀싱한 곡 이름은? 
################################################ 

  
SELECT ?result 
WHERE{ 
?x rdf:type iu:Song . 
  ?x iu:title ?result . 
  ?x iu:year ?2015 . 
  ?x iu:producedBy ?Singer . 
FILTER regex(str(?Singer),'아이유') . 
  }

  
