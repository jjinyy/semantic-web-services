WHERE{ 
  ?x iu:partOf ?Album . 
FILTER regex(str(?Album),'ModernTimes') . 
  ?x iu:composedBy ?PD . 
FILTER regex(str(?PD),'아이유') . 
?x rdf:type iu:Song . 
  ?x iu:title ?result . 
  }

